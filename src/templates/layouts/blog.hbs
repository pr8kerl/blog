<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
    <head>

    {{!-- Header --}}
    {{> header }}

    {{!-- main nav --}}
    {{> navbar-fixed-top }}

    {{!-- Content --}}
    <div class="container docs-container">
      <div class="row">
        <div class="col-md-3">
          <div class="sidebar hidden-print" data-spy="affix" role="complementary">
            <ul class="nav sidenav">
              {{#each pages }}  
              {{#isnt basename 'index' }}
                <li > <a  href ="{{ filename }}" > {{ data.title }} </a > </li > 
              {{/isnt}}
              {{/each  }}  
            </ul>
            <div id="toc">
            </div>
          </div>
        </div>
        <div class="col-md-9" role="main">
          <h1>{{ title }}</h1>
          {{#markdown}}
          {{> body }}
          {{/markdown}}
          <p>posted: {{date}}</p>
        </div>
      </div>
    </div>

    {{!-- Footer --}}
    {{> footer }}

    {{!-- JavaScript and analytics only. --}}
    {{> javascripts }}

  </body>
</html>
